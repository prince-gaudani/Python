# wap to print encodded no. increasing by 1 in each digit
# 0 to 8 --> increse 1 and 9 --> 0

n=int(input("Enter Number : "))
en_no=0
position=1

while n>0:
    digit=n%10
    n=n//10
    if digit<9:
        digit=digit+1
    else:
        digit=0
    en_no=en_no+(digit*position)
    position=position*10
